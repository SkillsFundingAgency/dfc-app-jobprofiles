@model EducationalRoute

<section class="job-profile-subsection"
         id="Apprenticeship">

    <h3>Apprenticeship</h3>

    <div class="job-profile-subsection-content">

        @Html.Raw(Model.RelevantSubjects)

        <h4>Entry requirements</h4>

        <ul class="list-reqs">
            @foreach (var requirement in Model.MoreInformation.Requirements)
            {
                <li><a class="htb-feature" href="#">@requirement</a></li>
            }
        </ul>

        <h4>More information</h4>

        <p>@Model.MoreInformation.Preamble</p>

        <ul class="list-link">
            @foreach (var readingLink in Model.MoreInformation.FurtherReading)
            {
                <li><a class="htb-feature" href="@readingLink.Link">@readingLink.Text</a></li>
            }
        </ul>
    </div>
</section>
