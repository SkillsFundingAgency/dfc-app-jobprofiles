@model ContentApiBranchElement
@{ 
    var collegeRrquirements = Model.ContentItems.Where(x => x.ContentType == "CollegeRequirement").ToList();
    var collegeLinks = Model.ContentItems.Where(x => x.ContentType == "CollegeLink").ToList();
    var requirementsPrefix = Model.ContentItems.Where(x => x.ContentType == "RequirementsPrefix").SingleOrDefault();
}

<section class="job-profile-subsection" id="College">
    <h3>College</h3>
    <div class="job-profile-subsection-content">
        @Html.Raw(Model.RelevantSubjects)

        <h4>Entry requirements</h4>
        <p>@requirementsPrefix.Content</p>
        <ul class="list-reqs">
            @foreach (var requirement in collegeRrquirements)
            {
                <li><a class="htb-feature" href="#">@requirement.Content</a></li>
            }
        </ul>

        <h4>More information</h4>
        <ul class="list-link">
            @foreach (var link in collegeLinks)
            {
                <li><a class="htb-feature" href="#">@link.Content</a></li>
            }
        </ul>
    </div>
</section>
