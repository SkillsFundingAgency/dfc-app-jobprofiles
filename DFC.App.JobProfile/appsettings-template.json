{
  "Logging": {
    "LogLevel": {
      "Default": "Error"
    }
  },
  "AllowedHosts": "*",
  "Configuration": {
    "DocumentStore": {
      "JobProfile": {
        "AccessKey": "[Access Key]",
        "EndpointUrl": "https://localhost:8081",
        "DatabaseId": "dfc-app-jobprofiles",
        "CollectionId": "jobProfiles",
        "PartitionKey": "/PartitionKey"
      },
      "CurrentOpportunities": {
        "AccessKey": "[Access Key]",
        "EndpointUrl": "https://localhost:8081",
        "DatabaseId": "dfc-app-jobprofiles",
        "CollectionId": "currentOpportunities",
        "PartitionKey": "/PartitionKey"
      },
      "OldCurrentOpportunities": {
        "AccessKey": "[Access Key]",
        "EndpointUrl": "https://localhost:8081",
        "DatabaseId": "dfc-app-jobprofiles",
        "CollectionId": "currentOpportunitiesSegments",
        "PartitionKey": "/PartitionKey"
      },
      "StaticContent": {
        "AccessKey": "[Access Key]",
        "EndpointUrl": "https://localhost:8081",
        "DatabaseId": "dfc-shared-content",
        "CollectionId": "A1_SC_Colin",
        "PartitionKey": "/PartitionKey"
      }
    }
  },
  "Policies": {
    "HttpCircuitBreaker": {
      "DurationOfBreak": "00:01:00",
      "ExceptionsAllowedBeforeBreaking": 3
    },
    "HttpRetry": {
      "BackoffPower": 2,
      "Count": 3
    }
  },
  "SubscriptionSettings": {
    "Endpoint": "https://localhost/api/webhook/ReceiveEvents",
    "SubscriptionServiceEndpoint": "https://localhost/api/execute/",
    "SubscriptionRegistrationDelay": "00:02:00",
    "Filter": {
      "IncludeEventTypes": [ "published", "unpublished", "deleted" ],
      "PropertyContainsFilters": [
        {
          "Key": "subject",
          "Values": [
            "/content/jobprofile/",
            "/content/sharedcontent/"
          ]
        }
      ]
    }
  },
  "FeedbackLinkConfiguration": {
    "SmartSurveyJP": "https://www.smartsurvey.co.uk/s/TestExploreCareersJobProfileFeedback/"
  },
  "ApplicationInsights": {
    "InstrumentationKey": "[instrumentation key]"
  },
  "ContentApiConfiguration": {
    "Timeout": "00:01:30",
    "ApiKey": "[API Key]",
    "EndpointUrl": "https://localhost:8081/",
    "SummaryEndpoint": "api/execute/jobprofile",
    "StaticContentEndpoint": "api/execute/sharedcontent/",
    "PageStaticContentIDs": [
      "you list of static content id's here..."
    ],
    "RelationshipStubs": [
      "HasRelatedCareer"
    ],
    "SupportedRelationships": [
      "HasRequirementsPrefix",
      "HasHtbUniversityRoute",
      "HasUniversityRequirement",
      "HasUniversityLink",
      "HasHtbCollegeRoute",
      "HasCollegeRequirement",
      "HasCollegeLink",
      "HasHtbApprenticeshipRoute",
      "HasApprenticeshipRequirement",
      "HasApprenticeshipLink",
      "HasHtbWorkRoute",
      "HasHtbVolunteeringRoute",
      "HasHtbDirectRoute",
      "HasHtbOtherRoute",
      "HasWitRestriction",
      "HasWitOtherRequirement",
      "HasONetOccupationalCode",
      "HasONetSkill",
      "HasWitDigitalSkillsLevel",
      "HasDayToDayTask",
      "HasWorkingEnvironment",
      "HasWorkingLocation",
      "HasWorkingUniform",
      "HasRelatedCareer"
    ]
  },
  "EventGridSubscriptionConfiguration": {
    "Timeout": "00:00:30",
    "ApiKey": "[APIM key]",
    "BaseAddress": "https://localhost/",
    "Endpoint": "api/execute/"
  }
}